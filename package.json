{
  "name": "todo",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "css": "postcss ./static/css/style.css -o ./static/css/compiled/style_compiled.css",
    "build_prod": "parcel build static/js/main.js --dist-dir static/builds/prod",
    "build_dev": "parcel watch static/js/main.js --dist-dir static/builds/dev"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "parcel": "^2.0.0-beta.2",
    "postcss-cli": "^7.1.1",
    "postcss-preset-env": "^6.7.0",
    "redux-devtools": "^3.7.0"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "defaults",
    "ie >= 10",
    "chrome >= 72",
    "firefox >= 60"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8"
  }
}
