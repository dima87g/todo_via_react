{
  "name": "todo",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "css": "postcss ./static/css/style.css -o ./static/css/compiled/style_compiled.css",
    "build_prod": "parcel build static/js/main.js --dist-dir static/builds/prod",
    "build_dev": "parcel watch static/js/main.js --dist-dir static/builds/dev"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "parcel": "^2.0.0-beta.2",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "redux-devtools": "^3.7.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "defaults",
    "ie >= 10",
    "chrome >= 72",
    "firefox >= 60"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9"
  }
}
